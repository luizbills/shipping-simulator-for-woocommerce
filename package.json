{
  "name": "wc-shipping-simulator",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm run minify:js && npm run minify:css",
    "minify:js": "esbuild assets/js/form.js --minify --outfile=assets/js/form.min.js",
    "minify:css": "esbuild assets/css/form.css --minify --outfile=assets/css/form.min.css"
  },
  "devDependencies": {
    "esbuild": "^0.14.49"
  }
}
